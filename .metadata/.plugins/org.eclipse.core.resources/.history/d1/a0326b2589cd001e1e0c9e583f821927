package com.example.tictactoe;

public class Move {
	private final int x,y;
	public int getX() {
		return x;
	}

	public int getY() {
		return y;
	}

	public Player getPlayer() {
		return player;
	}

	private final Player player;
	
	public Move(int x,int y, Player player) {
		this.x=x;
		this.y=y;
		this.player = player;
	}
	
	
}

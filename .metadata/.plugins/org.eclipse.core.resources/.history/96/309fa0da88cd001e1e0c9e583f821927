package com.example.tictactoe;

public class Player {
	private final User user;
	
	private final PieceType pieceType;
	
	public Player(User user, PieceType pieceType) {
		this.user = user;
		this.pieceType = pieceType;
	}

	public User getName() {
		return user;
	}

	public PieceType getPieceType() {
		return pieceType;
	}
 
}
